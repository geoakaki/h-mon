        <mxCell id="neostat-title" value="Heatmiser neoStat v2 Terminals" style="text;html=1;strokeColor=none;fillColor=#fff2cc;align=left;verticalAlign=middle;whiteSpace=wrap;rounded=1;fontSize=13;fontStyle=1" parent="1" vertex="1">
